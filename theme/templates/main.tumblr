<!DOCTYPE HTML>
<html>
<head>
	<!-- @include partials/title.tumblr -->

	<!-- @include partials/meta-open-graph.tumblr -->
	<link rel="stylesheet" type="text/css" href="">
	<script type="text/javascript" src=""></script>

	<link rel="shortcut icon" href="{Favicon}">
	<link rel="apple-touch-icon-precomposed" href="{PortraitURL-128}">
	<link rel="alternate" type="application/rss+xml" href="{RSS}">
	<link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Roboto:300,400,500,700" type="text/css">
	<link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

	<meta charset="utf-8">
	<meta name="viewport" content="width=750">

	{block:HasTags}
	<meta name="keywords" content="{block:Tags} {/block:Tags}"/>
	{/block:HasTags}

	<script src="//cdnjs.cloudflare.com/ajax/libs/require.js/2.1.11/require.min.js"></script>

	<!-- @include partials/meta-open-graph.tumblr -->
	<!-- @include partials/meta-customizer.tumblr -->

	<style>
		<!-- @include ../../build/theme.css -->
	</style>
	<script>
		<!-- @include ../../build/theme.js -->
	</script>

</head>
<body>
<a name="top">
	<div class="mdl-layout mdl-js-layout mdl-layout--fixed-drawer
		 mdl-layout--overlay-drawer-button">
		<div class="mdl-layout__drawer">
			<a href="{BlogURL}" title="{Title}"><img class="blog-avatar" src="{PortraitURL-64}" alt="{Title}"></a>
			<!-- @include partials/nav.tumblr -->
			<iframe width="350" height="500" id="prettycloud" class="tagcloud" frameborder="0" marginwidth="0" marginheight="0" allowtransparency="true" scrolling="no" src="http://post-theory.com/pretty-cloud/frame/gaming-thrones/c5194e" ></iframe>
			<a class="mdl-navigation__link" href="#top">Top</a>
		</div>
		<main class="mdl-layout__content">
			<div class="header">
				<div class="logo">
					<a href="http://{name}.tumblr.com/"><img src="http://static.tumblr.com/652e5f19256b84b73d7c106ba9098a05/rh4dbbq/vzJnrbq6y/tumblr_static_3ahlyjp0zu04sowg8k8c4scc8.jpg"/></a>
				</div>
			</div>

			<div class="content-wrapper">
				<div class="content">
					{block:IndexPage}
						<div class="description">
							{Description}
						</div>
					{/block:IndexPage}

					<!-- @include partials/block-search.tumblr -->

					<div class="posts">
						{block:Posts}
							<!-- @include partials/post-header.tumblr -->

							<div class="mdl-card__supporting-text">
								<!-- Begin post types -->
								<!-- @include partials/block-answer.tumblr -->
								<!-- @include partials/block-audio.tumblr -->
								<!-- @include partials/block-chat.tumblr -->
								<!-- @include partials/block-link.tumblr -->
								<!-- @include partials/block-panorama.tumblr -->
								<!-- @include partials/block-photo.tumblr -->
								<!-- @include partials/block-photoset.tumblr -->
								<!-- @include partials/block-quote.tumblr -->
								<!-- @include partials/block-text.tumblr -->
								<!-- @include partials/block-video.tumblr -->
								<!-- End post types -->
							</div>

							<!-- @include partials/post-footer.tumblr -->
						{/block:Posts}
						{block:IndexPage}
						<div class="pagination mdl-shadow--2dp mdl-grid">
							<div class="previous-page mdl-cell mdl-cell--4-col">
								{block:PreviousPage}
									<a href="{PreviousPage}">&#171; Previous</a>
								{/block:PreviousPage}
							</div>
							<div class="current-page mdl-cell mdl-cell--4-col">
								Page {CurrentPage} of {TotalPages}
							</div>
							<div class="next-page mdl-cell mdl-cell--4-col">
								{block:NextPage}
									<a href="{NextPage}">Next &#187;</a>
								{/block:NextPage}
							</div>
						</div>
						{/block:IndexPage}
					</div>
				</div>
			</div>
		</main>
	</div>
</body>
</html>
