<!DOCTYPE HTML>
<html>
<head>
	<!-- @include partials/title.tumblr -->

	<link rel="stylesheet" type="text/css" href="">

	<link rel="shortcut icon" href="{Favicon}">
	<link rel="apple-touch-icon-precomposed" href="{PortraitURL-128}">
	<link rel="alternate" type="application/rss+xml" href="{RSS}">

	<meta charset="utf-8">
	<meta name="viewport" content="width=750">

	{block:HasTags}
	<meta name="keywords" content="{block:Tags} {/block:Tags}" />
	{/block:HasTags}

	<!-- @include partials/meta-open-graph.tumblr -->
	<!-- @include partials/meta-customizer.tumblr -->
</head>
<body>
	<div class="container">
		<div class="header">
			<div class="logo">
				<a href="http://{name}.tumblr.com/"><img src="{image:Header}" /></a>
			</div>
			<nav class="top-bar" data-topbar>
				<ul class="title-area">
					<li class="name">
						<h1 class="title"><a href="http://{name}.tumblr.com/">{Title}</a></h1>
					</li>
					<!-- Remove the class "menu-icon" to get rid of menu icon. Take out "Menu" to just have icon alone -->
					<li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>
				</ul>

				<section class="top-bar-section">
					<!-- Right Nav Section -->
					<ul class="right">

						<!-- Custom Pages -->
						{block:Pages}
							<li><a href="{URL}">{Label}</a></li>
						{/block:Pages}

						<!-- Search Form -->
						<li class="has-form">
							<div class="row collapse">
								<div class="large-8 small-9 columns">
									<input type="text" placeholder="Find Stuff">
								</div>
								<div class="large-4 small-3 columns">
									<a href="#" class="alert button expand">Search</a>
								</div>
							</div>
						</li>
					</ul>
				</section>
			</nav>
		</div>

		<div class="content">
			<div class="description">
				{Description}
			</div>
			{block:Posts}
				<div class="posts">
					<!-- Begin post types -->

					<!-- @include partials/block-text.tumblr -->
					<!-- @include partials/block-photo.tumblr -->
					<!-- @include partials/block-panorama.tumblr -->
					<!-- @include partials/block-photoset.tumblr -->
					<!-- @include partials/block-quote.tumblr -->
					<!-- @include partials/block-link.tumblr -->
					<!-- @include partials/block-chat.tumblr -->
					<!-- @include partials/block-video.tumblr -->
					<!-- @include partials/block-audio.tumblr -->

					<!-- End post types -->
					<div class="back-top">
						<a href="#top">{lang:Back to top}</a>
					</div>
				</div> <!-- end .posts-->
			{/block:Posts}
		</div> <!-- end .content -->
		<div class="footer">
			{block:PreviousPage}
			<a href="{PreviousPage}">&#171; Previous</a>
			{/block:PreviousPage}{block:NextPage}
			<a href="{NextPage}">Next &#187;</a>
			{/block:NextPage}

			<a href="/archive">Archive</a>
		</div>
	</div> <!-- end .container -->

	<!-- Insert URL for theme.js here -->
	<script src=""></script>
</body>
</html>
