{block:Chat}
	<div class="post chat">
		<div class="header">
			{block:Title}
				<h2 class="title">
					<a href="{Permalink}">{Title}</a>
				</h2>
			{/block:Title}
		</div>

		<ul class="chat">
			{block:Lines}
				<li class="{Alt} user_{UserNumber}">
					{block:Label}
						<span class="label">{Label}</span>
					{/block:Label}{Line}
				</li>
			{/block:Lines}
		</ul>
		<!-- @include post-meta.tumblr -->
	</div>
{/block:Chat}
