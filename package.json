{
  "name": "gaming-thrones",
  "version": "0.1.0",
  "devDependencies": {
    "browserify-shim": "^3.5.0",
    "gulp-preprocess": "^1.1.1",
    "gulp-compass": "^1.1.9",
    "gulp-watch": "^0.6.8",
    "npm": "^1.4.16",
    "install": "^0.1.7",
    "gulp-browserify": "^0.5.0"
  },
  "dependencies": {
    "normalize.css": "~3.0.1"
  },
  "description": "tumblr-template-sass ====================",
  "main": "./theme/js/main.js",
  "browser": {
    "foundation": "./theme/libs/bower_components/foundation/js/foundation/foundation.js",
    "foundation.magellan": "./theme/libs/bower_components/foundation/js/foundation/foundation.magellan.js",
    "tumblrPhotoset": "./theme/libs/tumblr-photoset/tumblr-photoset.js",
    "tumblrPhotoset.albumPhotoset": "./theme/libs/tumblr-photoset/tumblr-photoset.album-photoset.js",
    "tumblrPhotoset.albumPhotoset.highres": "./theme/libs/tumblr-photoset/tumblr-photoset.album-photoset.highres.js",
    "tumblrPhotoset.lightbox": "./theme/libs/tumblr-photoset/tumblr-photoset.lightbox.js",
    "infiniteScroll": "./theme/libs/infinite-scroll/infinite-scroll.js"
  },
  "browserify-shim": {
    "jquery": "jQuery",
    "foundation": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "foundation.magellan": {
      "depends": [
        "jquery:jQuery",
        "foundation:foundation"
      ]
    },
    "infiniteScroll": {
      "depends": "jquery"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
